{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "Default": "Data Source=.;Initial Catalog=NanoHealth;integrated Security=true;MultipleActiveResultSets=true;Encrypt=false"
  },
  "JWT": {
    "Key": "this is my custom Secret key for authentication",
    "ValidIssuer": "https://localhost:7009/",
    "ValidAudience": "MySecureApiUser",
    "DurationInDays": "30"
  }
}
